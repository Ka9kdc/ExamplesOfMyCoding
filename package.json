    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-react": "^7.21.5",
    "ignore-styles": "^5.0.1",
    "mocha": "^8.2.0",
    "nodemon": "^2.0.6",
    "prettier": "2.1.2",
    "redux-thunk": "^2.3.0",
    "sinon": "^9.2.1",
    "style-loader": "^2.0.0",
    "supertest": "^6.0.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
