{
  "name": "WCRA",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "npm run start-server & npm run build-watch",
    "start-server": "nodemon server/index.js --ignore public",
    "build-watch": "webpack -w",
    "start": "npm run start:dev",
    "seed": "node script/seed.js",
    "test": "NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" --require @babel/polyfill --require @babel/register",
    "route-test": "NODE_ENV='test' mocha \"./server/routes/**/*.spec.js\" --require @babel/polyfill --require @babel/register",
    "model-test": "NODE_ENV='test' mocha \"./server/models/**/*.spec.js\" --require @babel/polyfill --require @babel/register",
    "redux-test": "NODE_ENV='test' mocha \"./client/redux/**/*.spec.js\" --require @babel/polyfill --require @babel/register",
    "react-test": "NODE_ENV='test' mocha \"./server/components/**/*.spec.js\" --require @babel/polyfill --require @babel/register",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "prettier": "npm run lint-fix && prettier --write ./ --ignore-path .gitignore"
  },
  "author": "Kelsey",
  "license": "ISC",
  "dependencies": {
    "@paypal/checkout-server-sdk": "^1.0.2",
    "connect-session-sequelize": "^7.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "pg": "^8.3.3",
    "react": "^16.13.1",
    "react-big-calendar": "^0.28.2",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "sequelize": "^6.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.12.1",
    "axios": "^0.20.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
